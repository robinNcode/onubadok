{
    "name": "robin/onubadok",
    "description": "This is composer package for translate your PHP software to other languages. Primarily in bengali language.",
    "type": "composer-plugin",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Robinncode\\Onubadok\\": "src/"
        }
    },
    "authors": [
        {
            "name": "MsM Robin",
            "email": "msmrobin518@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Robinncode\\Onubadok\\OnubadokServiceProvider"
            ]
        }
    }
}
